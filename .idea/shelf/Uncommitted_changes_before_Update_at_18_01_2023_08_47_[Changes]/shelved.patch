-        }
-
-        $ctx = hash_init($algo);
-        while (!$stream->eof()) {
-            hash_update($ctx, $stream->read(1048576));
-        }
-
-        $out = hash_final($ctx, $rawOutput);
-        $stream->seek($pos);
-
-        return $out;
-    }
