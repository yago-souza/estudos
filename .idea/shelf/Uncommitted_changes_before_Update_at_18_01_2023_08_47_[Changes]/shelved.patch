-{
-    /**
-     * Remove the items given by the keys, case insensitively from the data.
-     *
-     * @param string[] $keys
-     */
-    public static function caselessRemove(array $keys, array $data): array
-    {
-        $result = [];
-
-        foreach ($keys as &$key) {
-            $key = strtolower($key);
-        }
-
-        foreach ($data as $k => $v) {
-            if (!is_string($k) || !in_array(strtolower($k), $keys)) {
-                $result[$k] = $v;
-            }
-        }
-
-        return $result;
-    }
